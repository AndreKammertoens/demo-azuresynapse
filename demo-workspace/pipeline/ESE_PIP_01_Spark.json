					"enableLogging": false,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			},
			{
				"name": "Delete-CURATED-Data",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "Load-SQLPool",
						"dependencyConditions": [
							"Succeeded"
